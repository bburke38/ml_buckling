# test the wing udc

cfgpmtr vertical 1

despmtr tc1 0.04
despmtr tc2 0.02

despmtr c1 5.0
despmtr c2 2.0

despmtr dz1 0.0
despmtr dz2 0.0

despmtr x1 0.0
despmtr x2 1.0

despmtr y1 0.0
despmtr y2 3.0

despmtr twist1 0.0
despmtr twist2 0.0

udparg $/tail tc1 tc1 tc2 tc2
udparg $/tail c1 c1 c2 c2
udparg $/tail x1 x1 x2 x2
udparg $/tail dz1 dz1 dz2 dz2
udparg $/tail y1 y1 y2 y2
udparg $/tail twist1 twist1 twist2 twist2
udprim $/tail vertical vertical

end
